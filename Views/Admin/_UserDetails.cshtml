@model UserManagement.Models.UserDetail

<dl class="dl-horizontal">
    <dt>First Name</dt>
    <dd>@Model.FirstName</dd>

    <dt>Last Name</dt>
    <dd>@Model.LastName</dd>

    <dt>Date of Birth</dt>
    <dd>@Model.DOB.ToShortDateString()</dd>

    <dt>Gender</dt>
    <dd>@Model.Gender</dd>

    <dt>Email Address</dt>
    <dd>@Model.EmailAddress</dd>
</dl>
